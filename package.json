{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "app:build": "nuxt build",
    "app:dev": "nuxt dev",
    "app:run": "node ./.output/server/index.mjs",
    "docker:app:build": "docker build -t developer-log .",
    "docker:app:run": "docker run -p 3000:3000 developer-log",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "lint:staged": "lint-staged",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint",
    "lint:all": "concurrently -c green,yellow npm:lint:all:*",
    "lint:all:stylelint": "stylelint ./styles/**/*.scss ./**/*.vue --ip ./node_modules/**/*",
    "lint:all:eslint": "eslint --fix --ext .js,.vue,.ts components composables pages server data utils types layouts",
    "test:unit:run": "vitest run",
    "test:unit:gui": "vitest run --ui",
    "test:unit:watch": "vitest",
    "test:end:run": "playwright test",
    "test:end:gui": "playwright test --ui",
    "test:end:record": "playwright codegen",
    "test:end:setup": "playwright install",
    "deploy:dev": "vercel",
    "deploy:prod": "vercel --prod",
    "translate": "jsontt ./localization/ru.json --translator google --from ru --to be ko en kk"
  },
  "resolutions": {
    "micromark": "3.1.0",
    "mdast-util-to-string": "4.0.0"
  },
  "pnpm": {
    "overrides": {
      "node-fetch-native": "1.2.0",
      "unenv": "^1.7.4"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nuxt/content": "^2.7.2",
    "@nuxt/devtools": "latest",
    "@nuxt/image": "1.0.0-rc.1",
    "@nuxt/test-utils": "^3.7.0",
    "@nuxtjs/i18n": "8.0.0-rc.3",
    "@parvineyvazov/json-translator": "^1.7.0",
    "@playwright/test": "^1.37.1",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^18.17.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "axios": "^1.5.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.47.0",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-editorconfig": "^4.0.3",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-vitest": "^0.3.1",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "eslint-plugin-yml": "^1.8.0",
    "happy-dom": "^10.11.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.6.5",
    "nuxt-content-assets": "^1.3.2",
    "nuxt-icon": "^0.5.0",
    "nuxt-vitest": "^0.10.4",
    "postcss-html": "^1.5.0",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "vercel": "^32.0.1",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@fontsource-variable/montserrat": "^5.0.9",
    "@fontsource/iosevka": "^5.0.7",
    "@resvg/resvg-js": "^2.4.1",
    "axios": "^1.5.0",
    "csstype": "^3.1.2",
    "jsdom": "^22.1.0",
    "normalize.css": "^8.0.1",
    "satori": "^0.10.3",
    "satori-html": "^0.3.2",
    "sharp": "^0.32.5",
    "zod": "^3.22.2"
  }
}
